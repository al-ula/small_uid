{
  "name": "@al-ula/small-uid",
  "version": "0.3.3",
  "exports": {
    ".": "./mod.ts",
    "./pure": "./pure.ts",
    "./web": "./web.ts"
  },
  "tasks": {
    "prebuild": "deno run -A scripts/wasm_post_build.ts ./rng/  && deno run -A scripts/generate_web.ts mod.ts",
    "dev": "deno test --watch mod.ts",
    "build": "deno run -A scripts/build.ts"
  },
  "license": "MIT and Apache-2.0",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/fs": "jsr:@std/fs@^1.0.19"
  },
  "exclude": [
    ".github",
    ".npmignore",
    "package-lock.json",
    "pnpm-lock.yaml",
    "node_test.js",
    "*lock*"
  ],
  "fmt": {
    "exclude": ["dist", "rng"]
  },
  "lint": {
    "exclude": ["dist", "rng", "mod_test.ts", "mod_bench.ts"]
  },
  "publish": {
    "exclude": ["!rng/*", "!LICENSE*", "!web.ts"]
  }
}
